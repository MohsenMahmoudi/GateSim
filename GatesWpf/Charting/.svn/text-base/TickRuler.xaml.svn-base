<UserControl x:Class="GatesWpf.Charting.TickRuler"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Height="50">
    <StackPanel Orientation="Vertical">
        <Path Name="pLine" Stroke="Black" StrokeThickness="1" />
        <Grid Name="labels" />
        <TextBlock Name="labelType" TextAlignment="Center" HorizontalAlignment="Center" />
    </StackPanel>
    

</UserControl>
